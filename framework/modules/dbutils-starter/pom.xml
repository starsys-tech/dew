    <artifactId>dbutils-starter</artifactId>
    <name>1.2.5 Dew dbutils-Starter</name>
    <description>Dew 动态数据源实现</description>
    <packaging>jar</packaging>

    <properties>
    </properties>

    <dependencies>
        <dependency>
            <groupId>group.idealworld.dew</groupId>
