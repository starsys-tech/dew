        </dependency>
        <dependency>
            <groupId>group.idealworld.dew</groupId>
            <artifactId>cluster-common-test</artifactId>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-autoconfigure</artifactId>
        </dependency>
        <dependency>
            <groupId>group.idealworld.dew</groupId>
            <artifactId>test-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
